<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.zs.springcould.dao.MatchDao">

    <insert id="create" parameterType="com.zs.springcloud.entities.GameServer" useGeneratedKeys="true" keyProperty="id">
        insert into gameServer(serial) values(#{serial})
    </insert>


    <resultMap id="BaseResultMap" type="com.zs.springcloud.entities.GameServer">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <id column="serial" property="serial" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="getGameServerById" parameterType="Long" resultMap="BaseResultMap">
        select * from gameServer where id=#{id};
    </select>

</mapper>